msgstr "TOIMINTA"

#: mx-idesktool:65
msgid "DESCRIPTION"
msgstr "KUVAUS"

#: mx-idesktool:68
msgid "Middle-Click Command"
msgstr "Keskimmäisen hiirennapin napsautuksen käsky"

#: mx-idesktool:69
msgid "Set a precise position for an icon"
msgstr "Aseta kuvakkeen tarkka asema"

#: mx-idesktool:70
msgid "Directly edit an icon file"
msgstr "Mukauta kuvaketiedostoa suoraan"

#: mx-idesktool:71
msgid "Lock"
msgstr "Lukitse"

#: mx-idesktool:72
msgid "Permit/prevent icon movement"
msgstr "Evää/estä kuvakkeen liikkuminen"

#: mx-idesktool:73
msgid "Right-Click Command"
msgstr "Oikeanpuolen hiirennapin napsautuskäsky"

#: mx-idesktool:177 mx-idesktool:183
msgid "Link Filename"
msgstr "Linkitä tiedoston nimi"

#: mx-idesktool:178
msgid "Enter Link Filename"
msgstr "Syötä linkki-tiedosto nimi"

#: mx-idesktool:848
